{
    "name": "G1B",
    "short_name": "G1B",
    "description": "Aesthetic alternative to the default new tab.",
    "version": "0.0.7",
    "manifest_version": 2,
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "icons": {
      "128": "images/icon.png"
    },
    "web_accessible_resources": [
        "src/script.js",
        "src/styles.css",
        "images/icon.png"
    ]
 }
